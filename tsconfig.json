{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "lib": ["ES2020"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "rootDir": "src",
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "types": ["node", "express"]
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  